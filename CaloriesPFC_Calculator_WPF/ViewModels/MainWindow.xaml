<Window x:Class="CaloriesPFC_Calculator_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CaloriesPFC_Calculator_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <TabControl DockPanel.Dock="Top">
            <TabItem  Header="Add meals"></TabItem>
            <TabItem  Header="Today statistics"></TabItem>
            <TabItem  Header="All statistics"></TabItem>
            <TabItem  Header="Calculator"></TabItem>
        </TabControl>
    </DockPanel>
</Window>
